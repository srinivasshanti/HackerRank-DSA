import java.util.*;


class conversion
{
    public static void main(String a[])
    {
        Scanner sc=new Scanner(System.in);
        int dec=sc.nextInt();
        sc.close();

        //decimal to binary
        String bin=Integer.toBinaryString(dec);

        //binary to decimal
        System.out.println("binary to decimal: "+Integer.parseInt(bin,2));

        //decimal to Hexa
        String hex=Integer.toHexString(dec);

        //Hexa to decimal
        System.out.println("Hexa to decimal: "+Integer.parseInt(hex,16));

        //decimal to octal
        String octal=Integer.toOctalString(dec);

        //octal to decimal
        System.out.println("octal to decimal: "+Integer.parseInt(octal,8));
    }
}
