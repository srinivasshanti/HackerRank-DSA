

public class help {
	public static void main(String[] args)
	{
		String str1="abcd",str2="dbac";
		int count=0; 
		for(int i=0;i<str1.length();i++)
		{
			char str=str1.charAt(i);
			for(int j=0;j<str2.length();j++)
			{
				if(str==str2.charAt(j)) {
					count++; 
				}
			}			
		} 
		if(count==str1.length())
		{
		System.out.println("YES");
		}
	}
}
 
