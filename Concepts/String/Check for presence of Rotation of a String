
import java.util.*;

//suppose we have a string ABCD
//We need to check if rotation of ABCD is present or not

public class help {
	public static void main(String[] args)
	{
		String str1="ABCD";
		String str2="CDAf";
		 
		if((str1+str1).contains(str2)) {
			System.out.println("yes");
		}
		else {
			System.out.println("No");
		}
	}
 } 
