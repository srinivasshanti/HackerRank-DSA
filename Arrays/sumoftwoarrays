
public class sumoftwoarrays {
	
	static int find(int a1[],int a2[],int q)
	{ 
	  int sum=0;
		for(int i=0;i<4;i++)
		{
			for(int j=0;j<4;j++)
			{
				if(a1[i]+a2[j]==q)
				{
					sum=a1[i]+a2[j]; 
				} 
			} 			 
		}
		return sum;		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a1[]={10,8,9,5};
        int a2[]={5,9,8,10};
        int q=10;
        System.out.println(find(a1,a2,q));
	}

}
