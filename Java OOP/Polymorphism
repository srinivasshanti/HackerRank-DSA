package src;

public class poly {

	public static void main(String[] args) {
		CC ins=new CC();
		ins.m1();
		ins.m1(10);
		ins.m1("a");
	}
} 

class AA{
	public void m1() {
		System.out.println("Main loop");
	}
}


class BB extends AA{
	public void m1(int a) {
		System.out.println("B loop");
	}
}
 

class CC extends BB{
	public void m1(String a) {
		System.out.println("C loop");
	}
}
